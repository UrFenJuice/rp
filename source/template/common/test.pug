script(type='text/javascript').
	$(document).ready(function(){
	var block = $("#MainContent_ulThumbnail");
	var heightBlock = $("#large img");
	function resizeScreen(resizeBlock, heightBlock){
	block.height(function(){
	return heightBlock.height();
	});
	};
	resizeScreen(block, heightBlock);
	$(window).resize(function(){
	resizeScreen(block, heightBlock);
	});
	$("#MainContent_ulThumbnail li a").click(function(){
	$("#large img").hide().attr({"src": $(this).attr("href"), "title": $("> img", this).attr("title")});
	$("#large h2").html($("> img", this).attr("title"));
	return false;
	});
	$("#large>img").load(function(){$("#large>img:hidden").fadeIn("slow")});
	$("input[placeholder]").each(function () {
	//$(this).parent().prepend("<label for=\\"" + $(this).attr("id") + "\">" + $(this).attr("placeholder") + "</label>");
	//$(this).removeAttr("placeholder");
	if ($(this).val() == "") {
	$(this).parent().removeClass("active inactive").addClass("inactive");
	} else {
	$(this).parent().removeClass("active inactive").addClass("active");
	}
	$(this).focus(function () {
	$(this).parent().removeClass("active inactive").addClass("active");
	});
	$(this).blur(function () {
	if ($(this).val() == "") {
	$(this).parent().removeClass("active inactive").addClass("inactive");
	} else {
	$(this).parent().removeClass("active inactive").addClass("active");
	}
	});
	});
	});
